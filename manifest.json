{
  "name": "CSS Selector",
  "description": "Active query selector elements",
  "version": "1.0",
  "browser_action": {
      "default_title": "Query Selector.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
